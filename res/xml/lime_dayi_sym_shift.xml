<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	android:keyWidth="10%p" android:horizontalGap="0px"
	android:verticalGap="0px" android:keyHeight="@dimen/key_height">


	<Row>
		<Key android:codes="33" android:keyLabel="!\n言" 
			 android:keyEdgeFlags="left" />
		<Key android:codes="64" android:keyLabel="\@\n牛" />
		<Key android:codes="35" android:keyLabel="\#\n目" />
		<Key android:codes="36" android:keyLabel="$\n四" />
		<Key android:codes="37" android:keyLabel="%\n王" />
		<Key android:codes="94" android:keyLabel="^\n門" />
		<Key android:codes="38" android:keyLabel="&amp;\n田" />
		<Key android:codes="42" android:keyLabel="*\n米" />
		<Key android:codes="40" android:keyLabel="(\n足" />
		<Key android:codes="41" android:keyLabel=")\n金" android:keyEdgeFlags="right" />
	</Row>
	
	<Row>
		<Key android:codes="81" android:keyLabel="Q\n石" android:keyEdgeFlags="left" />
		<Key android:codes="87" android:keyLabel="W\n山" />
		<Key android:codes="69" android:keyLabel="E\n一" />
		<Key android:codes="82" android:keyLabel="R\n工" />
		<Key android:codes="84" android:keyLabel="T\n糸" />
		<Key android:codes="89" android:keyLabel="Y\n火" />
		<Key android:codes="85" android:keyLabel="U\n艸" />
		<Key android:codes="73" android:keyLabel="I\n木" />
		<Key android:codes="79" android:keyLabel="O\n口" />
		<Key android:codes="80" android:keyLabel="P\n耳" android:keyEdgeFlags="right" />
	</Row>
	
	<Row>
		<Key android:codes="65" android:keyLabel="A\n人" android:keyEdgeFlags="left" />
		<Key android:codes="83" android:keyLabel="S\n革" />
		<Key android:codes="68" android:keyLabel="D\n日" />
		<Key android:codes="70" android:keyLabel="F\n土" />
		<Key android:codes="71" android:keyLabel="G\n手" />
		<Key android:codes="72" android:keyLabel="H\n鳥" />
		<Key android:codes="74" android:keyLabel="J\n月" />
		<Key android:codes="75" android:keyLabel="K\n立" />
		<Key android:codes="76" android:keyLabel="L\n女" />
		<Key android:codes="58" android:keyLabel=":\n虫" android:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift"
			android:keyWidth="15%p" android:isModifier="true" android:isSticky="true"
			android:keyEdgeFlags="left" />
		<Key android:codes="90" android:keyLabel="Z\n心" />
		<Key android:codes="88" android:keyLabel="X\n水" />
		<Key android:codes="67" android:keyLabel="C\n鹿" />
		<Key android:codes="86" android:keyLabel="V\n禾" />
		<Key android:codes="66" android:keyLabel="B\n馬" />
		<Key android:codes="78" android:keyLabel="N\n魚" />
		<Key android:codes="77" android:keyLabel="M\n雨" />
		<Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:iconPreview="@drawable/sym_keyboard_feedback_delete" android:isModifier="true"
			android:keyWidth="15%p" android:keyEdgeFlags="right"
			android:isRepeatable="true" />
	</Row>
	<Row android:rowEdgeFlags="bottom">
		<Key android:codes="-3"  android:keyIcon="@drawable/sym_keyboard_done" android:iconPreview="@drawable/sym_keyboard_feedback_done" android:isModifier="true"
			android:keyWidth="15%p" android:keyEdgeFlags="left" />
		<Key android:codes="-9" 
				android:keyIcon="@drawable/sw" android:isModifier="true" 
				android:keyWidth="10%p" 
				android:iconPreview="@drawable/sw_l"/>
		<Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space_lime" android:iconPreview="@drawable/sym_keyboard_feedback_space"
			android:keyWidth="30%p" android:isRepeatable="true" />
		<Key android:codes="60" android:keyLabel="&lt;\n力" android:keyWidth="10%p" />
		<Key android:codes="62" android:keyLabel="&gt;\n舟"						 
								android:popupKeyboard="@xml/popup_punctuation"  
								android:keyWidth="10%p" />
		<Key android:codes="63" android:keyLabel="\?\n竹" android:keyWidth="10%p" />
		<Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:iconPreview="@drawable/sym_keyboard_feedback_return" android:isModifier="true"
			android:keyWidth="15%p" android:keyEdgeFlags="right" />
	</Row>

</Keyboard>
    